<h1>Edit Link</h1>

<%= form_for(@class_project, :url => update_project_path) do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :url %>
  <%= f.text_field :url %>

  <%= f.submit("Edit Link")%>
  
  <% @link.errors.full_messages.each do |msg| %>
      <p><%= msg %></p>
    <% end %>

<% end %>

